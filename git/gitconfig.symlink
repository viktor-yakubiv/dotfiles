[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes

[rerere]
	enabled = true

[pull]
	rebase = true

[init]
	defaultBranch = main

[includeIf "gitdir:~/Development/"]
	path = ~/.config/git/dev.gitconfig

# Local/private config goes in the include
# Overriding the current settings
[include]
	path = ~/.gitconfig.local
